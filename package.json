{
  "name": "simple-diff",
  "version": "0.0.1",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run parcel && npm run local",
    "local": "sed -i '' 's/\"index\\./\".\\/index\\./g' dist/index.html && sed -i '' 's/ type=\"module\"//g' dist/index.html",
    "parcel": "parcel build index.html --no-source-maps --public-url ./",
    "serve": "parcel serve index.html"
  },
  "dependencies": {
    "codemirror": "^5.61.0",
    "diff": "^5.0.0",
    "diff2html": "^3.4.4"
  },
  "devDependencies": {
    "@types/codemirror": "^5.60.0",
    "@types/diff": "^5.0.0",
    "parcel": "^2.0.0-nightly.750",
    "typescript": "^4.2.4"
  }
}
